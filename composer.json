{
    "name": "eyika/atom-framework",
    "description": "Core Libraries of Atom, An MVC microframework for PHP. Quite heavily inspired by Laravel",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "nesbot/carbon": "^3.7",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^3.7",
        "phpunit/phpunit": "^11.3",
        "predis/predis": "^2.2",
        "dragonmantank/cron-expression": "^3.3",
        "phpmailer/phpmailer": "^6.9",
        "eftec/bladeone": "^4.13",
        "eftec/bladeonehtml": "^2.4",
        "eftec/clione": "^1.33",
        "firebase/php-jwt": "^6.10",
        "league/flysystem": "^3.0",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-azure-blob-storage": "^3.0",
        "league/flysystem-google-cloud-storage": "^3.0",
        "voku/portable-ascii": "^2.0",
        "ramsey/uuid": "^4.7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Eyika\\Atom\\Framework\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Abdulbasit",
            "email": "basttyy@gmail.com"
        }
    ],
    "minimum-stability": "beta"
}
